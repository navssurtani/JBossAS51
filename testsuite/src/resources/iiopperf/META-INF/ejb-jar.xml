<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/j2ee/dtds/ejb-jar_2_0.dtd">

<ejb-jar>
   <description>Invocation Timing application</description>
   <display-name>Invocation Timing</display-name>
   <enterprise-beans>
      <session>
         <display-name>Session Bean for Invocation Timing</display-name>
         <ejb-name>InvocationTimingSession</ejb-name>
         <home>org.jboss.test.iiopperf.interfaces.SessionHome</home>
         <remote>org.jboss.test.iiopperf.interfaces.Session</remote>
         <ejb-class>org.jboss.test.iiopperf.ejb.SessionBean</ejb-class>
         <session-type>Stateful</session-type>
         <transaction-type>Container</transaction-type>
      </session>
   </enterprise-beans>
</ejb-jar>
