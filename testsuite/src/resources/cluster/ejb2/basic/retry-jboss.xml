      </session>
      
      <entity>
         <ejb-name>nextgen_EntityPK_Retry</ejb-name>
         <configuration-name>Retry Clustered CMP EntityBean</configuration-name>
         <invoker-bindings>
            <invoker>
               <invoker-proxy-binding-name>clustered-entity-rmi-invoker-retry</invoker-proxy-binding-name>
