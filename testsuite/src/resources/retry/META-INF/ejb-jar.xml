<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" 
                         "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
    <enterprise-beans>
        
       <session>
            <description>retry</description>
            <ejb-name>RetrySession</ejb-name>
            <home>org.jboss.test.retry.interfaces.RetrySessionHome</home>
            <remote>org.jboss.test.retry.interfaces.RetrySession</remote>
            <ejb-class>org.jboss.test.retry.bean.RetrySessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
    <assembly-descriptor>
    </assembly-descriptor>
</ejb-jar>

