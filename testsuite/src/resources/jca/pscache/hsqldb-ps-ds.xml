<datasources>

   <local-tx-datasource>
      <jndi-name>CachedPSDS</jndi-name>
      <connection-url>jdbc:hsqldb:${jboss.server.data.dir}${/}hypersonic${/}pscacheDB</connection-url>
      <driver-class>org.hsqldb.jdbcDriver</driver-class>
      <user-name>sa</user-name>
      <password></password>
      <min-pool-size>5</min-pool-size>
      <max-pool-size>20</max-pool-size>
      <prepared-statement-cache-size>2</prepared-statement-cache-size>
   </local-tx-datasource>

</datasources>