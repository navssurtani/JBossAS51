<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar >
   <enterprise-beans>
   
     <session>
        <ejb-name>CounterSession</ejb-name>
        <home>org.jboss.test.session.interfaces.CounterSessionHome</home>
        <remote>org.jboss.test.session.interfaces.CounterSession</remote>
        <ejb-class>org.jboss.test.session.ejb.CounterSessionBean</ejb-class>
        <session-type>Stateless</session-type>
        <transaction-type>Container</transaction-type>        
     </session>
     
     <session>
        <ejb-name>CountedSession</ejb-name>
        <home>org.jboss.test.session.interfaces.CountedSessionHome</home>
        <remote>org.jboss.test.session.interfaces.CountedSession</remote>
        <ejb-class>org.jboss.test.session.ejb.CountedSessionBean</ejb-class>
        <session-type>Stateless</session-type>
        <transaction-type>Container</transaction-type>        
     </session>
     
     <session>
        <ejb-name>CountedSession2</ejb-name>
        <home>org.jboss.test.session.interfaces.CountedSessionHome</home>
        <remote>org.jboss.test.session.interfaces.CountedSession</remote>
        <ejb-class>org.jboss.test.session.ejb.CountedSessionBean</ejb-class>
        <session-type>Stateless</session-type>
        <transaction-type>Container</transaction-type>        
     </session>
          
   </enterprise-beans>
</ejb-jar>
