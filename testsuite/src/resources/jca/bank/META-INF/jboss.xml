<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jboss PUBLIC "-//JBoss//DTD JBOSS 3.0//EN" "http://www.jboss.org/j2ee/dtd/jboss_3_0.dtd">

<jboss>

   <enterprise-beans>

     <!--
       To add beans that you have deployment descriptor info for, add
       a file to your XDoclet merge directory called jboss-beans.xml that contains
       the <session></session>, <entity></entity> and <message-driven></message-driven>
       markup for those beans.
     -->

      <entity>
         <ejb-name>Customer</ejb-name>
         <local-jndi-name>LocalCustomer</local-jndi-name>
         <configuration-name>Standard Pessimistic BMP EntityBean</configuration-name>

      </entity>
      <entity>
         <ejb-name>Account</ejb-name>
         <jndi-name>Account</jndi-name>
         <local-jndi-name>LocalAccount</local-jndi-name>
         <configuration-name>Standard Pessimistic BMP EntityBean</configuration-name>

      </entity>

      <session>
         <ejb-name>Teller</ejb-name>
         <jndi-name>Teller</jndi-name>
         <local-jndi-name>LocalTellerBean</local-jndi-name>

      </session>

   </enterprise-beans>

   <resource-managers>
   </resource-managers>

</jboss>
